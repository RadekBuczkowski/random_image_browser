    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="Xaml/MainWindow.xaml"
    Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
