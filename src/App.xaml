                <ResourceDictionary Source="Xaml/Styles/SlidingPanel.xaml" />
                <ResourceDictionary Source="Xaml/Styles/RadioButton.xaml" />
                <ResourceDictionary Source="Xaml/Styles/CheckBox.xaml" />
                <ResourceDictionary Source="Xaml/Styles/Button.xaml" />
                <ResourceDictionary Source="Xaml/Styles/TabControl.xaml" />
